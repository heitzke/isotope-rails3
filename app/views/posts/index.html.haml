- content_for(:title, "Blog &ndash; isotope|eleven".html_safe)

= interior_header("Blog<span id='rss_link'>#{link_to(image_tag('feed-icon-28x28.png'), posts_path(:atom))}</span>".html_safe)

.interior
  .fixed_content
    = render @posts
    = will_paginate @posts

    - if current_user
      = link_to 'New Post', new_post_path
