.fixed_content
  %h2.interior Login

.interior
  .fixed_content
    - form_for @user_session, :url => user_session_path do |f|
      = f.error_messages
      = f.label :username
      = f.text_field :username
      = f.label :password
      = f.password_field :password
      = f.check_box :remember_me
      = f.label :remember_me
      = f.submit "Login"
