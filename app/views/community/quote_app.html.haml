- content_for :title do
  quote_app &ndash; Community &ndash; isotope|eleven

= interior_header 'Community'

.interior
  .fixed_content
    = render_sibling_nav_for(:community)
    .work_content_left
      #github-readme
        Loading...
      %p
        = link_to 'Back to Community', '/community'
    .work_content_right
      = image_tag 'phase_zero.png', :alt => 'quote_app image'
      %ul
        %li.button
          = link_to('Github Repo', 'http://github.com/isotope11/quote_app')
        %li.button
          = link_to('Blog Post', 'http://isotope11.com/blog/isotope11-quote-app-open-source-quote-building-rails-project')
- content_for :javascripts do
  :javascript
    $(function(){
      $('#github-readme').load('/community/get_github_project?project=isotope11/quote_app #readme article');
    });
