- content_for :title do
  jenkins, Isotope style &ndash; Community &ndash; isotope|eleven

= interior_header 'Community'

.interior
  .fixed_content
    = render_sibling_nav_for(:community)
    .work_content_left
      #github-readme
        Loading...
      %p
        = link_to 'Back to Community', '/community'
    .work_content_right
      = link_to(image_tag('ruby.png', :size => "64x64",:alt => 'jenkins_isotope_style_image'), 'http://github.com/isotope11/jenkins-isotope-style')
      %ul
        %li
          = link_to('Github Repo', 'http://github.com/isotope11/jenkins-isotope-style')
- content_for :javascripts do
  :javascript
    $(function(){
      $('#github-readme').load('/community/get_github_project?project=isotope11/jenkins-isotope-style #readme article');
    });
