- content_for :title do
  Contract Acceptance Framework &ndash; Community &ndash; isotope|eleven

= interior_header 'Community'

.interior
  .fixed_content
    = render_sibling_nav_for(:community)
    .work_content_left
      #github-readme
        Loading...
      %p
        = link_to 'Back to Community', '/community'
    .work_content_right
      = image_tag 'phase_zero.png', :alt => 'contract_acceptance_framework image'
      %ul
        %li.button
          = link_to('RubyGem page', 'http://rubygems.org/gems/contract_acceptance_framework')
        %li.button
          = link_to('Github Repo', 'http://github.com/isotope11/contract_acceptance_framework')
- content_for :javascripts do
  :javascript
    $(function(){
      $('#github-readme').load('/community/get_github_project?project=isotope11/contract_acceptance_framework #readme article');
    });
